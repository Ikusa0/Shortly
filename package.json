{
  "name": "shortly-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:compose": "docker-compose -f docker-compose.development.yml --env-file .env.development",
    "dev:docker:up": "npm run dev:compose -- up -d && npm run dev:docker:logs",
    "dev:docker:down": "npm run dev:compose -- down --rmi all",
    "dev:docker:logs": "npm run dev:compose -- logs",
    "dev:docker:logs:follow": "npm run dev:compose -- logs -f",
    "dev": "NODE_ENV=development vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepare": "(husky && echo 'npx lint-staged' > .husky/pre-commit && echo '.git/hooks/commit-msg $1' > .husky/commit-msg) || true"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.6",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "git-commit-msg-linter": "^5.0.6",
    "husky": "^9.0.5",
    "lint-staged": "^15.2.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-svgr": "^4.2.0"
  }
}
